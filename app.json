{
  "expo": {
    "name": "GlaucoCare",
    "slug": "glaucocare",
    "scheme": "glaucocare",
    "owner": "feelcoder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/Logo-icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/Logo-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to capture profile photo and capture documents photos.",
        "NSPhotoLibraryUsageDescription": "This app needs access to photo library to select medical documents.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.glaucocare.glaucocare"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/Logo-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO",
        "android.permission.USE_BIOMETRIC",
        "android.permission.USE_FINGERPRINT"
      ],
      "package": "com.glaucocare.glaucocare",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1
    },
    "web": {
      "bundler": "metro",
      "favicon": "./src/assets/images/Logo-icon.png"
    },
    "plugins": [
      "expo-camera",
      "expo-notifications",
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow GlaucoCare to access your photos",
          "cameraPermission": "Allow GlaucoCare to access your camera"
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      "expo-local-authentication",
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "c6735223-ee6a-4651-ba98-b234173c1c77"
      }
    }
  }
}
